    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "file-type": "^16.2.0",
    "firebase-admin": "^9.4.2",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.4.1",
