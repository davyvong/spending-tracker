{
  "private": true,
  "scripts": {
    "build": "nest build",
    "eslint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --check --ignore-path .gitignore .",
    "start": "cross-env NODE_ENV=development nest start -e \"ts-node -r ./src/config\" --watch",
    "test": "cross-env NODE_ENV=test jest --coverage"
  },
  "dependencies": {
