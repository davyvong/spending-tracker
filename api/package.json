    "apollo-server-caching": "^0.7.0",
    "apollo-server-express": "^2.24.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express-jwt": "^6.1.0",
    "express-session": "^1.17.2",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.1.11",
    "graphql-shield": "^7.5.0",
